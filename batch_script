#!/bin/bash
#SBATCH --time=0-8:00
#SBATCH --job-name=isklearn
#SBATCH --cpus-per-task=32

DATASET="$1"
irace -s scenario.txt -p ${DATASET}/parameters.txt --parallel 32 --train-instances-file ${DATASET}/instances.txt --log-file "./output/${DATASET}_irace.Rdata"
